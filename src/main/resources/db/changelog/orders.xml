<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="orders" author="admin">
        <createTable tableName="orders">
            <column name="id" type="INT" defaultValueComputed="nextval('orders_seq')">
                <constraints primaryKey="true"/>
            </column>
            <column name="fioClient" type="VARCHAR(255)"/>
            <column name="email" type="VARCHAR(255)"/>
            <column name="carModel" type="VARCHAR(255)"/>
            <column name="VINCar" type="VARCHAR(255)"/>
            <column name="status" type="VARCHAR(255)"/>
            <column name="cost" type="INT"/>
        </createTable>
    </changeSet>
</databaseChangeLog>